{
	"compilerOptions": {
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"paths": {
			"#": ["./src/index.ts"],
			"#/*": ["./src/*"]
		}
	}
}
